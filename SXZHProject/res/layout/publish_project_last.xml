<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    >
	<include layout="@layout/title_bar" android:id="@+id/publish_project_last_bar"/>
	
	
		<TextView android:id="@+id/publish_project_report_last_contact_info"
		    		  android:layout_below="@+id/publish_project_last_bar"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="项目联系人信息"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					  android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"  		    
	   		    />
	  
	     
	   		
	   		<RelativeLayout android:id="@+id/publish_project_report_last_user_info_rel"
	    				android:layout_below="@+id/publish_project_report_last_contact_info"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
		   		<TextView android:id="@+id/publish_project_report_publish_name"
		   		    	  android:layout_width="80dip"
		   		    	  android:layout_height="wrap_content"	   		    	  
		   		    	  android:text="姓名："
						  android:textColor="@color/black"
						  android:gravity="right" 		    
		   		    />
		     	<TextView    android:layout_toRightOf="@+id/publish_project_report_publish_name"
		   		    	  android:layout_width="wrap_content"
		   		    	  android:layout_height="wrap_content"
		   		    	  android:text="testtest"
		   		    	  android:textSize="15sp"	
		   		    	  android:textColor="@color/black"
						  android:layout_centerVertical="true"	    
		   		    />
		</RelativeLayout>
		
	   		<RelativeLayout android:id="@+id/publish_project_report_last_user_phone_rel"
	    				android:layout_below="@+id/publish_project_report_last_user_info_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
		   		<TextView android:id="@+id/publish_project_report_publish_phone"
		   		    	  android:layout_width="80dip"
		   		    	  android:layout_height="wrap_content"	   		    	  
		   		    	  android:text="手机号码："
						  android:textColor="@color/black"
						  android:gravity="right" 		    
		   		    />
		     	<TextView    android:layout_toRightOf="@+id/publish_project_report_publish_phone"
		   		    	  android:layout_width="wrap_content"
		   		    	  android:layout_height="wrap_content"
		   		    	  android:text="135****8888"
		   		    	  android:textSize="15sp"	
		   		    	  android:textColor="@color/black"
						  android:layout_centerVertical="true"	    
		   		    />
		</RelativeLayout>
		
	   		<RelativeLayout android:id="@+id/publish_project_report_last_user_qq_rel"
	    				android:layout_below="@+id/publish_project_report_last_user_phone_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
		   		<TextView android:id="@+id/publish_project_report_publish_qq"
		   		    	  android:layout_width="80dip"
		   		    	  android:layout_height="wrap_content"	   		    	  
		   		    	  android:text="qq号码："
						  android:textColor="@color/black"
						  android:gravity="right" 	
						  android:layout_centerVertical="true"	    
		   		    />
		     	<EditText    android:layout_toRightOf="@+id/publish_project_report_publish_qq"
		   		    	  android:layout_width="fill_parent"
		   		    	  android:layout_height="wrap_content"
		   		    	  android:text="135****8888"
		   		    	  android:textSize="15sp"	
		   		    	  android:textColor="@color/black"
						  android:layout_centerVertical="true"	    
		   		    />
		</RelativeLayout>
		
	   		<TextView android:id="@+id/publish_project_report_last_rugutouzi"
		    		  android:layout_below="@+id/publish_project_report_last_user_qq_rel"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="入股投资"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					  android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"  		    
	   		    />
	   		
	   		
	   		<RelativeLayout android:id="@+id/publish_project_report_last_user_rugufangsi_rel"
	    				android:layout_below="@+id/publish_project_report_last_rugutouzi"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
					  android:background="@drawable/ic_setting_item_round_normal"
					  
		 	>
		   		<Button android:id="@+id/publish_project_report_publish_zhijierugu"
		   		    	  android:layout_width="wrap_content"
		   		    	  android:layout_height="wrap_content"	   		    	  
		   		    	  android:text="直接入资"
						  android:textColor="@color/black"
						  android:layout_marginTop="10dip"
						  android:layout_marginLeft="10dip"
						  	    
		   		    />
		     	<Button    android:layout_toRightOf="@+id/publish_project_report_publish_zhijierugu"
		   		    	  android:layout_width="wrap_content"
		   		    	  android:layout_height="wrap_content"
		   		    	  android:text="暂时观望"
		   		    	  android:textSize="15sp"	
		   		    	  android:layout_marginTop="10dip"
		   		    	  android:textColor="@color/black"
						 
		   		    />
		     	
		     	<TextView android:id="@+id/publish_project_report_publish_ruguinfo"
		   		    	  android:layout_width="fill_parent"
		   		    	  android:layout_height="100dip"	   		    	  
		   		    	  android:text="直接入资"
						  android:textColor="@color/black"
						  android:layout_marginTop="10dip"
						  android:layout_marginLeft="10dip"
						  android:layout_below="@+id/publish_project_report_publish_zhijierugu"
						  	    
		   		    />
		     	
		</RelativeLayout>
	  
	   		
	   		
	   		
	   		 <LinearLayout android:id="@+id/publish_project_last_xieyi_lin"
	         		   android:layout_below="@+id/publish_project_report_last_user_rugufangsi_rel"
	         		   android:layout_width="fill_parent"
	         		   android:layout_height="wrap_content"
	         		   android:layout_marginLeft="10dip"
	        	  	   android:layout_marginRight="10dip"
	        	  	   android:layout_marginTop="5dip"	 
	        	  	   android:orientation="horizontal"
	        	  	   android:gravity="center_vertical"
	         >
	         <CheckBox android:id="@+id/publish_project_last_xieyi" 	        	 
	        	  android:layout_width="wrap_content"
	        	  android:layout_height="wrap_content"	        	  
	        />
	     
	    	<TextView android:id="@+id/publish_project_last_xieyi_text" 
	        	  android:layout_width="wrap_content"
	        	  android:layout_height="wrap_content"
	        	  android:text="@string/publish_topic_xieyi"
	        	  android:textColor="@color/black"
	        />	         
	     </LinearLayout>
	     
	      <Button android:id="@+id/publish_project_last_submit"
	              android:layout_below="@+id/publish_project_last_xieyi_lin"
	          	  android:layout_width="fill_parent"
	          	  android:layout_height="wrap_content"
	          	  android:text="@string/publish_submit"
	          	  android:background="@drawable/ic_setting_item_round_normal" 
	          	  android:layout_marginLeft="10dip"
	        	  android:layout_marginRight="10dip"
	        	  android:layout_marginTop="5dip"
	        	  android:layout_marginBottom="5dip"
	          />
</RelativeLayout>
