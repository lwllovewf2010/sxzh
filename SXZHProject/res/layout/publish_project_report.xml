<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PublishOriginality" 
    android:background="@color/white"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >
	<include layout="@layout/title_bar4" android:id="@+id/publish_project_report_bar"/>
	<ScrollView 	android:layout_width="fill_parent"
	    			android:layout_height="fill_parent"
	    			android:layout_below="@+id/publish_project_report_bar">
	
	
		<RelativeLayout android:id="@+id/publish_project_report_content"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		 >
	   		<TextView android:id="@+id/publish_project_report_jinerxin"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="集资金额信息"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					  android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"  		    
	   		    />
	   		
	   		<RelativeLayout android:id="@+id/publish_project_report_projectcount_rel"
	    				android:layout_below="@+id/publish_project_report_jinerxin"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<EditText android:id="@+id/publish_project_report_projectcount"
	   		    	  android:layout_width="240dip"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="项目总金额，必填，整数"
					   		    
	   		    />
	     	<TextView    android:layout_toRightOf="@+id/publish_project_report_projectcount"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="RMB"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					 android:layout_centerVertical="true"	    
	   		    />
		</RelativeLayout>
	     
	   		
	   		<RelativeLayout android:id="@+id/publish_project_report_projectcountxiane_rel"
	    				android:layout_below="@+id/publish_project_report_projectcount_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<EditText android:id="@+id/publish_project_report_projectcountxiane"
	   		    	  android:layout_width="240dip"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="集资限额，必填，整数"
					   		    
	   		    />
	     	<TextView    android:layout_toRightOf="@+id/publish_project_report_projectcountxiane"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="RMB"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					  android:layout_centerVertical="true"	    
	   		    />
		</RelativeLayout>
		
	   		<RelativeLayout android:id="@+id/publish_project_report_day_rel"
	    				android:layout_below="@+id/publish_project_report_projectcountxiane_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<EditText android:id="@+id/publish_project_report_day"
	   		    	  android:layout_width="240dip"
	   		    	  android:layout_height="wrap_content"	
	   		    	  android:singleLine="true"   		    	  
	   		    	  android:hint=""
					   		    
	   		    />
	     	<TextView    android:layout_toRightOf="@+id/publish_project_report_day"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="RMB"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					 android:layout_centerVertical="true" 		    
	   		    />
		</RelativeLayout>
		
	   	<EditText android:id="@+id/publish_project_report_tuikuanshoum_rel"
	    				android:layout_below="@+id/publish_project_report_day_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="50dip"	    			
		    			android:hint="退款说明，必填，不超过75字"
		    			android:gravity="top"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	/>
	   		
	   	<TextView android:id="@+id/publish_project_report_huibaoxinxi_rel"
	    				android:layout_below="@+id/publish_project_report_tuikuanshoum_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:text="投资回报信息"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
					  android:textColor="@color/black"
					  android:textSize="15sp"
		 	/>
	   	
	   	
	   	<RelativeLayout android:id="@+id/publish_project_report_huibaotitle_rel"
	    				android:layout_below="@+id/publish_project_report_huibaoxinxi_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<EditText android:id="@+id/publish_project_report_huibaotitle"
	   		    	  android:layout_width="240dip"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="回报标题"
					   		    
	   		    />
	     	
		</RelativeLayout>
		
	   	<RelativeLayout android:id="@+id/publish_project_report_huibaotime_rel"
	    				android:layout_below="@+id/publish_project_report_huibaotitle_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<EditText android:id="@+id/publish_project_report_huibaotime"
	   		    	  android:layout_width="240dip"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="回报时间"
					   		    
	   		    />
	     	<TextView    android:layout_toRightOf="@+id/publish_project_report_huibaotime"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="天"
	   		    	  android:textSize="15sp"	
	   		    	  android:textColor="@color/black"
					  android:layout_centerVertical="true"	    
	   		    />
		</RelativeLayout>
		
	   	<EditText android:id="@+id/publish_project_report_huinbaocontent_rel"
	    				android:layout_below="@+id/publish_project_report_huibaotime_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="80dip"	    			
		    			android:hint="回报内容"
		    			android:gravity="top"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	/>
	   	
	   	
	
	
		
		<RelativeLayout android:id="@+id/publish_project_report_xiandingrenshu_rel"
	    				android:layout_below="@+id/publish_project_report_huinbaocontent_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
		 	>
	   		<Button android:id="@+id/publish_project_report_buxianding"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="不限定入股人数"
					   		    
	   		    />
	     	<Button android:id="@+id/publish_project_report_xianding"   
	     	    	android:layout_toRightOf="@+id/publish_project_report_buxianding"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"
	   		    	  android:text="限定人数"
	   		    	     
	   		    />
	     	<EditText android:layout_width="60dip"
	     	    	  android:layout_height="wrap_content"
	     	    	  android:id="@+id/publish_project_renchu"	
	     	    	  android:layout_toRightOf="@+id/publish_project_report_xianding"
	     	    />
	     	<TextView android:layout_width="wrap_content"
	     	    	  android:layout_height="wrap_content"
	     	    	  android:id="@+id/publish_project_renchu_title"	
	     	    	  android:layout_toRightOf="@+id/publish_project_renchu"
	     	    	  android:text="人"
	     	    	  android:textColor="@color/black"
	     	    	 android:layout_centerVertical="true"
	     	    />
		</RelativeLayout>
		 
		<TextView android:layout_width="wrap_content"
	     	    	  android:layout_height="wrap_content"
	     	    	  android:id="@+id/publish_project_kuaidi_fangsi_title"	
	     	    	  android:layout_below="@+id/publish_project_report_xiandingrenshu_rel"
	     	    	  android:text="支持邮寄方式："
	     	    	  android:textColor="@color/black"
	     	    	 android:layout_centerVertical="true"
	     	    	 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
	     	    />
		
		<LinearLayout android:id="@+id/publish_project_report_kuaidifangsi_linear"
	    				android:layout_below="@+id/publish_project_kuaidi_fangsi_title"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="horizontal"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
					 
		 	>
	   		<Button android:id="@+id/publish_project_report_kuaidi"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="快递"
					  android:layout_weight="1.0" 		    
	   		    />
	     	<Button android:id="@+id/publish_project_report_ems"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="EMS"
					  android:layout_weight="1.0" 		    
	   		    />
	     	<Button android:id="@+id/publish_project_report_bubaoyou"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="不包邮"
					  android:layout_weight="1.0" 		    
	   		    />
		</LinearLayout>>
		
		
		
		<RelativeLayout android:id="@+id/publish_project_report_zhuanli_rel"
	    				android:layout_below="@+id/publish_project_report_kuaidifangsi_linear"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
					  android:background="@drawable/ic_setting_item_round_normal"
		 	>
	   		<Button android:id="@+id/publish_project_report_youfaming"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="没有发明专利"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"					  		    
	   		    />
	   		<Button android:id="@+id/publish_project_report_meiyoufaming"
	   		    	  android:layout_toRightOf="@+id/publish_project_report_youfaming"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="有发明专利"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"					  		    
	   		    />
	   		<EditText android:id="@+id/publish_project_report_zhuanlibianhao"
	   		    	  android:layout_toRightOf="@+id/publish_project_report_youfaming"
	   		    	  android:layout_width="fill_parent"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="发明专利编号"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"
					  android:layout_marginRight="5dip"					  		    
	   		    />
	   		<EditText android:id="@+id/publish_project_report_zhuanliname"
	   		    	  android:layout_toRightOf="@+id/publish_project_report_zhuanlibianhao"
	   		    	  android:layout_width="fill_parent"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="发明专利名称"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"
					  android:layout_marginRight="5dip"						  		    
	   		    />
		</RelativeLayout>
		
		
		
		<RelativeLayout android:id="@+id/publish_project_report_gufenzi_rel"
	    				android:layout_below="@+id/publish_project_report_zhuanli_rel"
		    			android:layout_width="fill_parent"
		    			android:layout_height="wrap_content"	    			
		    			android:orientation="vertical"
		    			 android:layout_marginLeft="10dip"
					  android:layout_marginTop="10dip"
					  android:layout_marginRight="10dip"
					  android:background="@drawable/ic_setting_item_round_normal"
		 	>
	   		<Button android:id="@+id/publish_project_report_gufenzi"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="全部股份均分"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"					  		    
	   		    />
	   		<Button android:id="@+id/publish_project_report_yougufenzi"
	   		    	  android:layout_toRightOf="@+id/publish_project_report_gufenzi"
	   		    	  android:layout_width="wrap_content"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="有股票数限制"
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"					  		    
	   		    />
	   		<TextView android:id="@+id/publish_project_report_gufentitle"
	   		    	  android:layout_below="@+id/publish_project_report_gufenzi"
	   		    	  android:layout_width="fill_parent"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:hint="我占股份的百分比："
	   		    	  android:layout_marginLeft="5dip"
					  android:layout_marginTop="5dip"
					  android:layout_marginRight="5dip"					  		    
	   		    />
	   		<EditText android:id="@+id/publish_project_report_zhuanliname"
	   		    	  android:layout_toRightOf="@+id/publish_project_report_gufentitle"
	   		    	  android:layout_width="60dip"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	 					  		    
	   		    />
	   		<TextView 
	   		    	  android:layout_toRightOf="@+id/publish_project_report_zhuanliname"
	   		    	  android:layout_width="fill_parent"
	   		    	  android:layout_height="wrap_content"	   		    	  
	   		    	  android:text="%"
	   		    	  				  		    
	   		    />
		</RelativeLayout>
		
		</RelativeLayout>
	</ScrollView>
	
</RelativeLayout>
